@model List<DomainCompany.Entities.CustomPage>

@foreach (var itemTitle in Model)
{
    <div class="col-md-2 col-institucional">
        <b>@itemTitle.Name </b>
        @* <div class="ui inverted link list"> *@
            <ul>
            @foreach (var itemPage in itemTitle.CustomPagesContent)
            {
                <li><a href="/institucional/@itemTitle.IdCustomPage/@itemPage.IdCustomPageContent" class="item" title="@itemPage.Name">@itemPage.Name</a></li>
            }
            </ul>
            <ul>
            @if (@itemTitle.Name.ToUpper() == "DÚVIDAS")
            {
                <li><a href="@Url.Action("CommonQuestions", "Company")" class="item" title="Dúvidas Frequentes">Dúvidas Frequentes</a></li>
            }
            </ul>
        @* </div> *@
    </div>
    
}




